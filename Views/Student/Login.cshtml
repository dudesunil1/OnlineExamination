@model OnlineExamination.Models.StudentMasterModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <!-- Title -->
    <title>Online Examination</title>
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Content/assets/images/logo/favicon.png">
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap.min.css">
    <!-- file upload -->
    <link rel="stylesheet" href="~/Content/assets/css/file-upload.css">
    <!-- file upload -->
    <link rel="stylesheet" href="~/Content/assets/css/plyr.css">
    <!-- DataTables -->
    <link rel="stylesheet" href="https://cdn.datatables.net/2.0.8/css/dataTables.dataTables.min.css">
    <!-- full calendar -->
    <link rel="stylesheet" href="~/Content/assets/css/full-calendar.css">
    <!-- jquery Ui -->
    <link rel="stylesheet" href="~/Content/assets/css/jquery-ui.css">
    <!-- editor quill Ui -->
    <link rel="stylesheet" href="~/Content/assets/css/editor-quill.css">
    <!-- apex charts Css -->
    <link rel="stylesheet" href="~/Content/assets/css/apexcharts.css">
    <!-- calendar Css -->
    <link rel="stylesheet" href="~/Content/assets/css/calendar.css">
    <!-- jvector map Css -->
    <link rel="stylesheet" href="~/Content/assets/css/jquery-jvectormap-2.0.5.css">
    <!-- Main css -->
    <link rel="stylesheet" href="~/Content/assets/css/main.css">
</head>
<body>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <!--==================== Preloader Start ====================-->
        <div class="preloader">
            <div class="loader"></div>
        </div>
        <!--==================== Preloader End ====================-->
        <!--==================== Sidebar Overlay End ====================-->
        <div class="side-overlay"></div>
        <!--==================== Sidebar Overlay End ====================-->

        <section class="auth d-flex">
            <div class="auth-left bg-main-50 flex-center p-24">
                <img src="~/Content/assets/images/thumbs/auth-img1.png" alt="">
            </div>
            <div class="auth-right py-40 px-24 flex-center flex-column">
                <div class="auth-right__inner mx-auto w-100">
                    <a href="index.html" class="auth-right__logo">
                        <img src="~/Content/assets/images/logo/logo.png" alt="">
                    </a>
                    <h2 class="mb-8">Welcome to Student! &#128075;</h2>
                    <p class="text-gray-600 text-15 mb-32">Please sign in to your account and start the adventure</p>

                    <form action="#">
                        <div class="mb-24">
                            @Html.Label("Username", htmlAttributes: new { @class = "form-label mb-8 h6" })
                            <div class="position-relative">
                                @Html.TextBoxFor(model => model.Stud_UserName, new { @id = "fname", @class = "form-control py-11 ps-40" })
                                <span class="position-absolute top-50 translate-middle-y ms-16 text-gray-600 d-flex"><i class="ph ph-user"></i></span>
                            </div>
                        </div>

                        <div class="mb-24">
                            @Html.Label("Password", htmlAttributes: new { @class = "form-label mb-8 h6" })

                            <div class="position-relative">
                                @Html.TextBoxFor(model => model.Stud_Password, new { @id = "current-password", @class = "form-control py-11 ps-40" })


                                <span class="toggle-password position-absolute top-50 inset-inline-end-0 me-16 translate-middle-y ph ph-eye-slash"></span>
                                <span class="position-absolute top-50 translate-middle-y ms-16 text-gray-600 d-flex">
                                    <i class="ph ph-lock"></i>
                                </span>

                               
                            </div>
                        </div>
                       

                        <input type="submit" value="Sign In" class="btn btn-main rounded-pill w-100" />
                        @*<asp:Button ID="btnSignIn" runat="server" Text="Sign In" CssClass="btn btn-main rounded-pill w-100" OnClick="btnSignIn_Click" />*@
                    </form>
                </div>
            </div>
        </section>
    }
    <!-- Jquery js -->
    <script src="~/Content/assets/js/jquery-3.7.1.min.js"></script>
    <!-- Bootstrap Bundle Js -->
    <script src="~/Content/assets/js/boostrap.bundle.min.js"></script>
    <!-- Phosphor Js -->
    <script src="~/Content/assets/js/phosphor-icon.js"></script>
    <!-- file upload -->
    <script src="~/Content/assets/js/file-upload.js"></script>
    <!-- file upload -->
    <script src="~/Content/assets/js/plyr.js"></script>
    <!-- dataTables -->
    <script src="https://cdn.datatables.net/2.0.8/js/dataTables.min.js"></script>
    <!-- full calendar -->
    <script src="~/Content/assets/js/full-calendar.js"></script>
    <!-- jQuery UI -->
    <script src="~/Content/assets/js/jquery-ui.js"></script>
    <!-- jQuery UI -->
    <script src="~/Content/assets/js/editor-quill.js"></script>
    <!-- apex charts -->
    <script src="~/Content/assets/js/apexcharts.min.js"></script>
    <!-- jvectormap Js -->
    <script src="~/Content/assets/js/jquery-jvectormap-2.0.5.min.js"></script>
    <!-- jvectormap world Js -->
    <script src="~/Content/assets/js/jquery-jvectormap-world-mill-en.js"></script>

    <!-- main js -->
    <script src="~/Content/assets/js/main.js"></script>
</body>
</html>
